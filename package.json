{
  "name": "noc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --respawn --clear --env-file=.env src/app.ts",
    "dev:debug": "tsnd --respawn --clear --env-file=.env --inspect=9229 src/app.ts",
    "build": "rimraf ./dist && tsc",
    "start": "npm run build && node dist/app.js",
    "test": "jest --env-file=.env.test",
    "test:watch": "jest --detectOpenHandles --watch --env-file=.env.test",
    "test:coverage": "jest --coverage --detectOpenHandles --env-file=.env.test",
    "test:debug": "node --inspect-brk --env-file=.env.test ./node_modules/.bin/jest",
    "docker:test": "docker-compose -f docker-compose.test.yml --env-file=.env.test up -d"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.1",
    "jest": "^30.1.1",
    "prisma": "^6.15.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "cron": "^4.3.3",
    "dotenv": "^17.2.1",
    "env-var": "^7.5.0",
    "mongoose": "^8.18.0",
    "nodemailer": "^7.0.5"
  }
}
